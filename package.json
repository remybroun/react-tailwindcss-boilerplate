    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "react-router-dom": "^5.3.3",
    "react-scripts": "^4.0.3",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
